{
  "name": "HeliSwap-yield-farming",
  "version": "1.0.0",
  "description": "HeliSwap Yield Farming Contracts",
  "main": "index.js",
  "scripts": {
    "deployFactory": "npx hardhat deployFactory",
    "deployMultirewards": "npx hardhat deployMultirewards --factoryaddress 0x0000000000000000000000000000000002dc6d03 --owner 0.0.47795264 --stakingtoken 0x0000000000000000000000000000000002dc6fbe",
    "deploy": "npx hardhat deploy --owner 0x00000000000000000000000000000000020cf0a3 --stakingtoken 0x1165f6115eb38eD34E93B303f9388a901b6d47d4",
    "addReward": "npx hardhat addReward --contractaddress 0x0000000000000000000000000000000002Dc72eE --rewardaddress 0x0000000000000000000000000000000002dc6fa8 --rewarddistributor 0.0.47795264 --rewardduration 86400",
    "sendReward": "npx hardhat sendReward --contractaddress 0x0000000000000000000000000000000002Dc72eE --rewardaddress 0x0000000000000000000000000000000002dc6fa8 --rewardamount 10000 --rewarddecimals 8",
    "approveERC20": "npx hardhat approveERC20 --tokenid 0x0000000000000000000000000000000002dc6fbe --spender 0x0000000000000000000000000000000002Dc72eE --amount 100000000000 --lender 0.0.47795264 --lenderpk 0x981fda8cf4ac64adb31e387351729bfd3feef99ad127b83d31135a7c5b56371e",

    "associateToken": "npx hardhat associateToken --accountid 0.0.47795264 --pk 0x981fda8cf4ac64adb31e387351729bfd3feef99ad127b83d31135a7c5b56371e --tokenid 0.0.48000936",
    "approveToken": "npx hardhat approveToken --accountid 0.0.47795264 --pk 0x981fda8cf4ac64adb31e387351729bfd3feef99ad127b83d31135a7c5b56371e --spenderaccountid 0x0000000000000000000000000000000002Dc72eE --tokenid 00.0.48000936 --amount 100000000000000",
    "stake": "npx hardhat stake --contractaddress 0x0000000000000000000000000000000002Dc72eE --amount 100000",
    "withdraw": "npx hardhat withdraw --contractaddress 0x0000000000000000000000000000000002Dc72eE --amount 100000",
    "exit": "npx hardhat exit --contractaddress 0x0000000000000000000000000000000002Dc72eE"
  },
  "repository": "https://github.com/LimeChain/HeliSwap-yield-farming",
  "author": "Martin Dobrev",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "@ethersproject/abi": "^5.4.7",
    "@ethersproject/providers": "^5.4.7",
    "@hashgraph/hardhat-hethers": "^1.0.0",
    "@hashgraph/hethers": "^1.2.0",
    "@hashgraph/sdk": "^2.17.1",
    "@nomicfoundation/hardhat-chai-matchers": "^1.0.0",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.0",
    "@nomicfoundation/hardhat-toolbox": "^1.0.1",
    "@nomiclabs/hardhat-ethers": "^2.0.0",
    "@nomiclabs/hardhat-etherscan": "^3.0.0",
    "@typechain/ethers-v5": "^10.1.0",
    "@typechain/hardhat": "^6.1.2",
    "@types/chai": "^4.2.0",
    "@types/mocha": "^9.1.0",
    "@types/node": ">=12.0.0",
    "chai": "^4.2.0",
    "ethers": "^5.4.7",
    "hardhat": "^2.10.1",
    "hardhat-gas-reporter": "^1.0.8",
    "solidity-coverage": "^0.7.21",
    "ts-node": ">=8.0.0",
    "typechain": "^8.1.0",
    "typescript": ">=4.5.0"
  }
}
